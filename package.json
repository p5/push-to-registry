    "ini": "^4.1.2"
  },
  "devDependencies": {
    "@redhat-actions/action-io-generator": "^1.5.0",
    "@redhat-actions/eslint-config": "^1.3.2",
    "@redhat-actions/tsconfig": "^1.2.0",
    "@types/ini": "^4.1.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "typescript": "5.3"
  }
